#include<iostream>
using namespace std;
void partition(int array[],int high, int low , int pivot)  //naive partition 
{
    int temp[high-low+1];
    int index;
    for(int i=low;i<=high;i++)
    {
         if(array[i]<=array[pivot])
         { 
            temp[index]=array[i];
            index++;
         }
    }
    for(int i=low;i<=high;i++)
    {
        if(array[i]>=array[pivot])
        {
            temp[index]=array[i];
            index++;
        }
    }   
    for(int i=0;i<=high;i++)
    {
        array[i]=temp[i];
    }
}
int main()
{
    int arr[7]={5,13,6,9,12,11,8};
    partition(arr,6,0,6);
    for(int i=0;i<7;i++)
       cout<<arr[i]<<" ";
}
